{
    "version": 2,
    "builds": [
      {
        "src": "apps/api/next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "apps/web/next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "^/api/(.*)",
        "dest": "apps/api/$1"
      },
      {
        "src": "^/(.*)",
        "dest": "apps/web/$1"
      }
    ]
}
  